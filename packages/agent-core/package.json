{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "0.2.0",
  "name": "@adolago/agent-core",
  "description": "CLI + daemon powering the Personas system (Zee, Stanley, Johny)",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adolago/agent-core.git",
    "directory": "packages/agent-core"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "lint": "prettier --check 'src/**/*.ts' 'test/**/*.ts' 'script/**/*.ts' 'package.json'",
    "test": "bun test",
    "build": "bun run script/build.ts && ln -sf $(pwd)/dist/@adolago/agent-core-linux-x64/bin/agent-core ~/.bun/bin/agent-core",
    "dev": "bun run --conditions=browser ./src/index.ts",
    "memory:health": "bun run script/memory-health-check.ts",
    "clean": "rm -rf node_modules dist"
  },
  "bin": {
    "agent-core": "./bin/agent-core"
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "7.6.1",
    "@parcel/watcher-linux-arm64-glibc": "2.5.1",
    "@parcel/watcher-linux-arm64-musl": "2.5.1",
    "@parcel/watcher-linux-x64-glibc": "2.5.1",
    "@parcel/watcher-linux-x64-musl": "2.5.1",
    "@parcel/watcher-win32-x64": "2.5.1",
    "@tsconfig/bun": "1.0.9",
    "@types/bun": "1.3.5",
    "@types/turndown": "5.0.5",
    "@types/yargs": "17.0.33",
    "@typescript/native-preview": "7.0.0-dev.20251207.1",
    "typescript": "5.8.2",
    "vscode-languageserver-types": "3.17.5"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1",
    "@agentclientprotocol/sdk": "0.5.1",
    "@ai-sdk/anthropic": "2.0.57",
    "@ai-sdk/google": "2.0.52",
    "@ai-sdk/openai": "2.0.89",
    "@ai-sdk/openai-compatible": "2.0.18",
    "@ai-sdk/provider": "3.0.3",
    "@ai-sdk/provider-utils": "4.0.6",
    "@babel/core": "7.28.0",
    "@clack/prompts": "1.0.0-alpha.1",
