    "@openauthjs/openauth": "0.0.0-20250322224806",
    "@openrouter/ai-sdk-provider": "1.5.2",
    "@opentui/core": "0.1.75",
    "@opentui/solid": "0.1.75",
    "@parcel/watcher": "2.5.1",
    "@pierre/diffs": "1.0.2",
    "@qdrant/js-client-rest": "1.16.2",
    "@solid-primitives/event-bus": "1.1.2",
