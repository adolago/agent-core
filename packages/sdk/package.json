{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@agent-core/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Agent Core daemon API",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "build": "tsgo"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./v2": "./src/v2/index.ts",
    "./v2/client": "./src/v2/client.ts",
    "./v2/server": "./src/v2/server.ts"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "@typescript/native-preview": "catalog:"
  },
  "dependencies": {},
  "publishConfig": {
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adolago/agent-core.git",
    "directory": "packages/sdk"
  },
  "keywords": [
    "agent-core",
    "sdk",
    "ai",
    "llm",
    "typescript"
  ]
}
