{
  "name": "@agent-core/core",
  "version": "0.1.0",
  "description": "Unified AI Agent Core - Powers Stanley (GUI), Zee (messaging), and OpenCode (CLI/TUI)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.js"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js"
    },
    "./tool": {
      "types": "./dist/tool/index.d.ts",
      "import": "./dist/tool/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    },
    "./memory": {
      "types": "./dist/memory/index.d.ts",
      "import": "./dist/memory/index.js"
    },
    "./surface": {
      "types": "./dist/surface/index.d.ts",
      "import": "./dist/surface/index.js"
    },
    "./session": {
      "types": "./dist/session/index.d.ts",
      "import": "./dist/session/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./transport": {
      "types": "./dist/transport/index.d.ts",
      "import": "./dist/transport/index.js"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "import": "./dist/util/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier --write src",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@qdrant/js-client-rest": "^1.12.0",
    "ai": "^4.3.0",
    "decimal.js": "^10.4.0",
    "eventsource-parser": "^3.0.0",
    "fuzzysort": "^3.0.0",
    "glob": "^13.0.0",
    "jsonc-parser": "^3.3.1",
    "nanoid": "^5.0.0",
    "p-queue": "^8.0.1",
    "picomatch": "^4.0.3",
    "remeda": "^2.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/google": "^1.2.0",
    "@ai-sdk/openai": "^1.3.0",
    "@openrouter/ai-sdk-provider": "^0.4.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.17.0",
    "prettier": "^3.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@ai-sdk/amazon-bedrock": "^1.0.0",
    "@ai-sdk/azure": "^1.0.0",
    "@ai-sdk/cerebras": "^1.0.0",
    "@ai-sdk/cohere": "^1.0.0",
    "@ai-sdk/deepinfra": "^1.0.0",
    "@ai-sdk/gateway": "^1.0.0",
    "@ai-sdk/google-vertex": "^1.0.0",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/mistral": "^1.0.0",
    "@ai-sdk/openai-compatible": "^0.1.0",
    "@ai-sdk/perplexity": "^1.0.0",
    "@ai-sdk/togetherai": "^1.0.0",
    "@ai-sdk/xai": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^0.4.0"
  },
  "engines": {
    "node": ">=20"
  },
  "author": "Agent Core Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/artur/agent-core"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "opencode",
    "stanley",
    "zee",
    "mcp",
    "qdrant",
    "claude",
    "openai",
    "anthropic",
    "chatbot",
    "assistant",
    "multi-provider",
    "subscription-auth"
  ]
}
