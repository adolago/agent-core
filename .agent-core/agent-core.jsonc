{
  "$schema": "agent-core",
  "default_agent": "zee",
  "model": "zai-coding-plan/glm-4.7",
  "small_model": "zai-coding-plan/glm-4.7",
  "instructions": ["STYLE_GUIDE.md"],
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp"
    },
    "memory": { "enabled": true },
    "calendar": { "enabled": true },
    "portfolio": { "enabled": true }
  },
  "memory": {
    "qdrant": {
      "url": "http://localhost:6333",
      "collection": "personas_memory"
    },
    "embedding": {
      "profile": "nebius/qwen3-embedding-8b",
      "dimensions": 4096,
      "baseUrl": "https://api.tokenfactory.nebius.com/v1",
      "apiKey": "{env:NEBIUS_API_KEY}"
    }
  },
  "tiara": {
    "qdrant": {
      "url": "http://localhost:6333",
      "stateCollection": "personas_state",
      "memoryCollection": "personas_memory",
      "embeddingDimension": 4096
    }
  },
  "zee": {
    "splitwise": {
      "enabled": true,
      "token": "{env:SPLITWISE_TOKEN}"
    }
  },
  "tools": {
    "github-triage": false,
    "github-pr-search": false
  },
  "tui": {
    "dictation": {
      "enabled": true,
      "model": "default",
      "region": "us-central1",
      "language": "en-US",
      "alternative_languages": ["pt-BR", "es-ES", "de-DE"]
    }
  },
  "agent": {
    "zee": {
      "description": "Personal assistant for life admin, memory, messaging, and calendar",
      "mode": "primary",
      "color": "#5c9cf5",
      "model": "zai-coding-plan/glm-4.7"
    },
    "stanley": {
      "description": "Investing and financial research assistant",
      "mode": "primary",
      "color": "#f5a742"
    },
    "johny": {
      "description": "Study assistant for learning and knowledge graphs",
      "mode": "primary",
      "color": "#7fd88c"
    },
    "title": {
      "model": "zai-coding-plan/glm-4.7",
      "temperature": 0.5,
      "hidden": true
    },
    "compaction": {
      "model": "zai-coding-plan/glm-4.7",
      "temperature": 0.3,
      "hidden": true
    }
  }
}
