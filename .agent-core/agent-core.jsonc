{
  "$schema": "agent-core",
  "default_agent": "zee",
  "model": "zai-coding-plan/glm-4.7",
  "small_model": "zai-coding-plan/glm-4.7",
  "instructions": ["STYLE_GUIDE.md"],
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp"
    },
    "memory": { "enabled": true },
    "calendar": { "enabled": true },
    "portfolio": { "enabled": true }
  },
  "memory": {
    "qdrant": {
      "url": "http://localhost:6333",
      "collection": "personas_memory"
    },
    "embedding": {
      "profile": "google/gemini-embedding-001",
      "dimensions": 3072,
      "apiKey": "{env:GEMINI_API_KEY}"
    }
  },
  "tiara": {
    "qdrant": {
      "url": "http://localhost:6333",
      "stateCollection": "personas_state",
      "memoryCollection": "personas_memory",
      "embeddingDimension": 3072
    }
  },
  "zee": {
    "splitwise": {
      "enabled": true,
      "token": "{env:SPLITWISE_TOKEN}"
    }
  },
  "provider": {
    "kimi-for-coding": {
      "options": {
        "baseURL": "https://api.kimi.com/coding/v1"
      }
    },
    "xai": {
      "options": {
        "baseURL": "https://api.x.ai/v1"
      }
    },
    "vllm": {
      "options": {
        "baseURL": "http://192.168.178.102:8000/v1"
      }
    }
  },
  // "disabled_providers": [],
  "fallback": {
    "enabled": true,
    "rules": [
      {
        "condition": "rate_limit",
        "fallbacks": ["kimi-for-coding/kimi-k2.5-thinking"]
      },
      {
        "condition": "error",
        "fallbacks": ["kimi-for-coding/kimi-k2.5-thinking"]
      }
    ]
  },
  "tools": {
    "github-triage": false,
    "github-pr-search": false
  },
  "tui": {
    "dictation": {
      "enabled": true,
      "model": "default",
      "region": "us-central1",
      "language": "en-US",
      "alternative_languages": ["pt-BR", "es-ES", "de-DE"]
    }
  },
  "agent": {
    "zee": {
      "description": "Personal assistant for life admin, memory, messaging, and calendar",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "primary",
      "color": "#5c9cf5"
    },
    "stanley": {
      "description": "Investing and financial research assistant",
      "model": "xai/grok-4-1-fast",
      "mode": "primary",
      "color": "#f5a742"
    },
    "johny": {
      "description": "Study assistant for learning and knowledge graphs",
      "model": "anthropic/claude-opus-4.5",
      "mode": "primary",
      "color": "#7fd88c"
    },
    "title": {
      "model": "zai-coding-plan/glm-4.7",
      "temperature": 0.5,
      "hidden": true
    },
    "compaction": {
      "model": "zai-coding-plan/glm-4.7",
      "temperature": 0.3,
      "hidden": true
    }
  }
}
